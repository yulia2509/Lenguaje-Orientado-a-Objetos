import java.util.Scanner;


public class Division { 

  public static void main(String[] args) {  
	
	Scanner sc = new Scanner(System.in); //Ya declaramos al "lector"
	
	//Se le pide el numero al usuario
	System.out.println("Por favor ingrese el numero 1");
	float numero1 = sc.nextFloat();
	
	System.out.println("Por favor ingrese el numero 2");
	float numero2 = sc.nextFloat();
	
	float result = numero1/numero2;
	System.out.println("El resultado de la division es "+ result);
	
  }
  
}
