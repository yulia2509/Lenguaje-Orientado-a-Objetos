package restaurante;

/**
 *
 * @author Yulia
 */
public class Mesero extends Empleado {
    private int tip;
    
    public Mesero(){
        super();
    }
    
    public Mesero(String name, String SSN, int age, int weight, int tip){
        super(name, SSN, age, weight);
        this.tip = tip;
    }
    
    public void setTip(int tip) {
        this.tip = tip;
    }
    public int getTip() {
        return tip;
    }
    
    public void tomarOrden(){
        System.out.println(" est√° tomando la orden");
    }
}
