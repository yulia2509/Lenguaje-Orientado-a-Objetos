package restaurante;

/**
 *
 * @author Yulia
 */
public class Empleado {
    protected String name;
    protected String SSN;
    protected int age;
    protected int weight;
    
    public Empleado(){
    
    }
    
    public Empleado(String name, String SSN, int age, int weight){
        this.name = name;
        this.SSN = SSN;
        this.age = age;
        this.weight = weight;
    }
    
    public void setName(String name) {
        this.name = name;
    }
    public String getName() {
        return name;
    }
    
    public void setSSN(String SSN) {
        this.SSN = SSN;
    }
    public String getSSN() {
        return SSN;
    }
    
    public void setAge(int age) {
        this.age = age;
    }
    public int getAge() {
        return age;
    }
    
    public void setPeso(int weight) {
        this.weight = weight;
    }
    public int getPeso() {
        return weight;
    }
    
    public void Descansar(){
        System.out.println(" está descansando");
    }
    
    public void Limpiar(){
        System.out.println(" está limpiando");
    }
    
    public void Comer(){
        System.out.println(" está comiendo");
    }
}
