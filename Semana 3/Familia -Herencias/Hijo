public class Hijo extends Humano {
    
    public Hijo(){
        super();
    }
    
    public Hijo (String name, int age, float weight){
        super(name, age, weight);
    }
    
    public void irEscuela(){
        System.out.println(" fue a la escuela");
    }
    
    public void Jugar(){
        System.out.println(" est√° jugando");
    }
    
}
