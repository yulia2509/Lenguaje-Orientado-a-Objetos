package familia;

/**
 *
 * @author Yulia
 */
public class Mama extends Humano {
    private boolean peloLargo;
    
    public Mama(){
        super();
    }
    
    public Mama (String name, int age, float weight, boolean peloLargo){
        super(name, age, weight);
        this.peloLargo = peloLargo;
    }
    
    public void setPelo(boolean peloLargo) {
        this.peloLargo = peloLargo;
    }
    public boolean getPelo() {
        return peloLargo;
    }
    
    public void Maquillar(){
        System.out.println(" se maquilla");
    }
    
}
